NO-1
GROUP:	Volume Indicators
NAME:	AD
HINT:	Chaikin A/D Line
CAMELCASENAME:	Ad
FLAGS:
INPUT PARAMETER 1
	TYPE:	Price
	NAME:	inPriceHLCV
	FLAGS:	HIGH	LOW	CLOSE	VOLUME
OUTPUT PARAMETER 1
	TYPE:	Real
	NAME:	outReal
	FLAGS:

NO-2
GROUP:	Volume Indicators
NAME:	ADOSC
HINT:	Chaikin A/D Oscillator
CAMELCASENAME:	AdOsc
FLAGS:
INPUT PARAMETER 1
	TYPE:	Price
	NAME:	inPriceHLCV
	FLAGS:	HIGH	LOW	CLOSE	VOLUME
OPT INPUT PARAMETER 1
	TYPE:	IntegerRange [2, 4, 1, 200, 100000]
	NAME:	optInFastPeriod
	DEFAULT:	3
	DISPLAY NAME:	Fast Period
	HINT:	Number of period for the fast MA
	HELP FILE:	(null)
OPT INPUT PARAMETER 2
	TYPE:	IntegerRange [2, 4, 1, 200, 100000]
	NAME:	optInSlowPeriod
	DEFAULT:	10
	DISPLAY NAME:	Slow Period
	HINT:	Number of period for the slow MA
	HELP FILE:	(null)
OUTPUT PARAMETER 1
	TYPE:	Real
	NAME:	outReal
	FLAGS:

