NO-5
GROUP:	Momentum Indicators
NAME:	APO
HINT:	Absolute Price Oscillator
CAMELCASENAME:	Apo
FLAGS:
INPUT PARAMETER 1
	TYPE:	Real
	NAME:	inReal
	FLAGS:
OPT INPUT PARAMETER 1
	TYPE:	IntegerRange [2, 4, 1, 200, 100000]
	NAME:	optInFastPeriod
	DEFAULT:	12
	DISPLAY NAME:	Fast Period
	HINT:	Number of period for the fast MA
	HELP FILE:	(null)
OPT INPUT PARAMETER 2
	TYPE:	IntegerRange [2, 4, 1, 200, 100000]
	NAME:	optInSlowPeriod
	DEFAULT:	26
	DISPLAY NAME:	Slow Period
	HINT:	Number of period for the slow MA
	HELP FILE:	(null)
OPT INPUT PARAMETER 3
	TYPE:	IntegerList
		0:	SMA
		1:	EMA
		2:	WMA
		3:	DEMA
		4:	TEMA
		5:	TRIMA
		6:	KAMA
		7:	MAMA
		8:	T3
	NAME:	optInMAType
	DEFAULT:	0
	DISPLAY NAME:	MA Type
	HINT:	Type of Moving Average
	HELP FILE:	(null)
OUTPUT PARAMETER 1
	TYPE:	Real
	NAME:	outReal
	FLAGS:

