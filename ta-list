NO-10
GROUP:	Overlap Studies
NAME:	BBANDS
HINT:	Bollinger Bands
CAMELCASENAME:	Bbands
FLAGS:	OVERLAP
INPUT PARAMETER 1
	TYPE:	Real
	NAME:	inReal
	FLAGS:
OPT INPUT PARAMETER 1
	TYPE:	IntegerRange [2, 4, 1, 200, 100000]
	NAME:	optInTimePeriod
	DEFAULT:	5
	DISPLAY NAME:	Time Period
	HINT:	Number of period
	HELP FILE:	(null)
OPT INPUT PARAMETER 2
	TYPE:	RealRange [-3e+37, -2, 0.2, 2, 3e+37]
	NAME:	optInNbDevUp
	DEFAULT:	2
	DISPLAY NAME:	Deviations up
	HINT:	Deviation multiplier for upper band
	HELP FILE:	(null)
OPT INPUT PARAMETER 3
	TYPE:	RealRange [-3e+37, -2, 0.2, 2, 3e+37]
	NAME:	optInNbDevDn
	DEFAULT:	2
	DISPLAY NAME:	Deviations down
	HINT:	Deviation multiplier for lower band
	HELP FILE:	(null)
OPT INPUT PARAMETER 4
	TYPE:	IntegerList
		0:	SMA
		1:	EMA
		2:	WMA
		3:	DEMA
		4:	TEMA
		5:	TRIMA
		6:	KAMA
		7:	MAMA
		8:	T3
	NAME:	optInMAType
	DEFAULT:	0
	DISPLAY NAME:	MA Type
	HINT:	Type of Moving Average
	HELP FILE:	(null)
OUTPUT PARAMETER 1
	TYPE:	Real
	NAME:	outRealUpperBand
	FLAGS:
OUTPUT PARAMETER 2
	TYPE:	Real
	NAME:	outRealMiddleBand
	FLAGS:
OUTPUT PARAMETER 3
	TYPE:	Real
	NAME:	outRealLowerBand
	FLAGS:

